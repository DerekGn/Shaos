{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=shaos.db;"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning",
        "Microsoft.AspNetCore.Mvc": "Warning",
        "Microsoft.AspNetCore.Routing": "Warning",
        "Microsoft.AspNetCore.Hosting": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ]
  },
  "PageSize": 5,
  "AllowedHosts": "*",
  "FileStoreOptions": {
    "PackagesPath": "C:\\temp\\Shaos\\Packages",
    "BinariesPath": "C:\\temp\\Shaos\\Binaries"
  }
}
